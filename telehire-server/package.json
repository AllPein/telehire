{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "migrate:dev": "npx prisma migrate dev",
    "migrate:dev:create": "npx prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "prepare": "is-ci || husky install",
    "prisma:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@casl/prisma": "^1.4.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "10.2.5",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.5",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.2.6",
    "@nestjs/passport": "10.0.2",
    "@nestjs/platform-express": "10.2.5",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "7.1.11",
    "@prisma/client": "^5.3.1",
    "@twa.js/init-data-node": "^0.0.11",
    "axios": "^1.5.1",
    "cache-manager": "^5.2.3",
    "cache-manager-ioredis-yet": "^1.2.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.6",
    "ioredis": "^5.3.2",
    "ms": "2.1.3",
    "nest-winston": "^1.9.4",
    "nestjs-cls": "^3.5.1",
    "nestjs-prisma": "^0.22.0",
    "nestjs-telegraf": "^2.7.0",
    "passport": "0.6.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "snakecase-keys": "^5.4.7",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "5.0.0",
    "telegraf": "^4.14.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.17",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "^10.2.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.86",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.5",
    "@types/ms": "0.7.31",
    "@types/node": "18.17.17",
    "@types/passport-jwt": "3.0.9",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "env-cmd": "10.1.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "3.0.3",
    "prisma": "^5.3.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}